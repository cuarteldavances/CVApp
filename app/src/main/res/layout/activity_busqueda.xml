<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="cv.andevelopnica.com.crediveloz.BusquedaActivity"
    android:background="#ffffff">
    <TextView
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:id="@+id/textView3"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:background="#dddddd" />

    <ImageView
        android:layout_width="195dp"
        android:layout_height="60dp"
        android:id="@+id/imageView2"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="5dp"
        android:background="@mipmap/logomaincv" />

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView3"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:id="@+id/relativeLayout3"
        android:layout_marginTop="3dp">


    </RelativeLayout>



    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/relativeLayout3"
        android:layout_centerHorizontal="true"
        android:layout_marginRight="5dp"
        android:layout_marginLeft="5dp">

        <ImageView
            android:layout_width="match_parent"
            android:layout_height="34dp"
            android:id="@+id/imageView11"
            android:layout_centerVertical="true"
            android:layout_centerHorizontal="true"
            android:background="@mipmap/detalledecuentacv" />

    </RelativeLayout>







    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/relativeLayout3"
        android:layout_centerHorizontal="true"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:id="@+id/relativeLayout4"
        android:layout_marginTop="40dp"
        android:layout_marginBottom="2dp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Nombre:"
            android:id="@+id/textView"
            android:textColor="@color/abc_primary_text_material_light"
            android:textStyle="bold"
            android:layout_alignBottom="@+id/btn_buscarnom"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <AutoCompleteTextView
            android:layout_width="180dp"
            android:layout_height="50dp"
            android:id="@+id/edt_nombre"
            android:inputType="textPersonName"
            android:hint="Nombre del Cliente"
            android:textStyle="normal"
            android:textSize="12sp"
            android:textColor="#000000"
            android:layout_alignParentTop="true"
            android:layout_toRightOf="@+id/textView"
            android:layout_toEndOf="@+id/textView" />

        <Button
            android:layout_width="70dp"
            android:layout_height="28dp"
            android:text="Buscar"
            android:id="@+id/btn_buscarnom"
            android:typeface="normal"
            android:textStyle="bold"
            android:textColor="#FFFFFF"
            android:background="#002C83"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            android:textSize="12sp"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true" />

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/relativeLayout4"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:layout_marginTop="0dp"
        android:id="@+id/relativeLayout5"
        android:layout_marginBottom="2dp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="DirecciÃ³n:"
            android:id="@+id/textView2"
            android:layout_centerVertical="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginTop="2dp"
            android:textColor="@color/abc_primary_text_material_light"
            android:layout_marginRight="5dp"
            android:textStyle="bold" />

        <EditText
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:id="@+id/edt_direccion"
            android:inputType="textPersonName"
            android:textStyle="normal"
            android:textSize="12sp"
            android:textColor="#000000"
            android:layout_alignTop="@+id/textView2"
            android:layout_toRightOf="@+id/textView2"
            android:layout_toEndOf="@+id/textView2" />

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/relativeLayout5"
        android:layout_centerHorizontal="true"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:layout_marginTop="2dp"
        android:id="@+id/relativeLayout6"
        android:layout_marginBottom="2dp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Telefono:"
            android:id="@+id/textView7"
            android:layout_centerVertical="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:textColor="@color/abc_primary_text_material_light"
            android:layout_marginTop="5dp"
            android:layout_marginRight="5dp"
            android:textStyle="bold" />

        <EditText
            android:layout_width="110dp"
            android:layout_height="50dp"
            android:id="@+id/edt_telefono"
            android:inputType="textPersonName"
            android:textStyle="normal"
            android:textSize="12sp"
            android:textColor="#000000"
            android:layout_alignParentTop="true"
            android:layout_toRightOf="@+id/textView7" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Monto:"
            android:id="@+id/textView10"
            android:textColor="@color/abc_primary_text_material_light"
            android:textStyle="bold"
            android:layout_alignTop="@+id/textView7"
            android:layout_toRightOf="@+id/edt_telefono"
            android:layout_toEndOf="@+id/edt_telefono" />

        <EditText
            android:layout_width="90dp"
            android:layout_height="50dp"
            android:id="@+id/edt_monto"
            android:inputType="textPersonName"
            android:textStyle="normal"
            android:textSize="12sp"
            android:textColor="#000000"
            android:layout_alignParentTop="true"
            android:layout_toRightOf="@+id/textView10"
            android:layout_toEndOf="@+id/textView10" />

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/relativeLayout6"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="2dp"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:id="@+id/relativeLayout8"
        android:layout_marginBottom="2dp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Ultimo Abono:"
            android:id="@+id/textView9"
            android:textColor="@color/abc_primary_text_material_light"
            android:textStyle="bold"
            android:layout_centerVertical="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <EditText
            android:layout_width="120dp"
            android:layout_height="30dp"
            android:id="@+id/edt_ultiabono"
            android:inputType="textPersonName"
            android:textStyle="normal"
            android:textSize="12sp"
            android:textColor="#000000"
            android:layout_alignParentTop="true"
            android:layout_toRightOf="@+id/textView9" />

        <EditText
            android:layout_width="60dp"
            android:layout_height="30dp"
            android:id="@+id/edt_cuota"
            android:inputType="textPersonName"
            android:textStyle="normal"
            android:textSize="12sp"
            android:textColor="#000000"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Cuota:"
            android:id="@+id/textView5"
            android:textColor="@color/abc_primary_text_material_light"
            android:textStyle="bold"
            android:layout_alignBaseline="@+id/edt_ultiabono"
            android:layout_alignBottom="@+id/edt_ultiabono"
            android:layout_toRightOf="@+id/edt_ultiabono"
            android:layout_toEndOf="@+id/edt_ultiabono" />

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/relativeLayout8"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:id="@+id/relativeLayout9"
        android:layout_marginBottom="15dp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Saldo:"
            android:id="@+id/textView13"
            android:textColor="@color/abc_primary_text_material_light"
            android:textStyle="bold"
            android:layout_alignBaseline="@+id/tv_idcartera"
            android:layout_alignBottom="@+id/tv_idcartera"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <EditText
            android:layout_width="90dp"
            android:layout_height="50dp"
            android:id="@+id/edt_saldo"
            android:inputType="textPersonName"
            android:textStyle="normal"
            android:textSize="12sp"
            android:textColor="#000000"
            android:layout_alignBaseline="@+id/textView13"
            android:layout_alignBottom="@+id/textView13"
            android:layout_toRightOf="@+id/textView13"
            android:layout_toEndOf="@+id/textView13" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="ID Cartera:"
            android:id="@+id/tv_idcartera"
            android:textColor="@color/abc_primary_text_material_light"
            android:textStyle="bold"
            android:layout_marginLeft="25dp"
            android:layout_marginStart="25dp"
            android:layout_centerVertical="true"
            android:layout_toRightOf="@+id/edt_saldo"
            android:layout_toEndOf="@+id/edt_saldo" />

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="0dp"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:id="@+id/relativeLayout11"
        android:layout_below="@+id/relativeLayout9"
        android:layout_marginBottom="0dp">

        <Button
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:text="Hist. Abonos"
            android:id="@+id/btn_bbuscarnom"
            android:typeface="normal"
            android:textStyle="bold"
            android:textColor="#FFFFFF"
            android:background="#002C83"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            android:textSize="12sp"
            android:layout_centerVertical="true"
            android:layout_centerHorizontal="true" />
    </RelativeLayout>


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="55dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:background="#dddddd"
        android:id="@+id/relativeLayout39">

        <ImageView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/imageView"
            android:background="@mipmap/bottomcv4"
            android:layout_alignBottom="@+id/cobros"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <Button
            android:layout_width="60dp"
            android:layout_height="wrap_content"
            android:text="@string/pagosbtn"
            android:id="@+id/pagos"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentBottom="true"
            android:alpha="0" />

        <Button
            android:layout_width="60dp"
            android:layout_height="wrap_content"
            android:text="cobros"
            android:id="@+id/cobros"
            android:alpha="0"
            android:layout_alignParentTop="true"
            android:layout_toRightOf="@+id/pagos"
            android:layout_marginLeft="8dp" />

        <Button
            android:layout_width="60dp"
            android:layout_height="wrap_content"
            android:text="desembolso"
            android:id="@+id/desembolso"
            android:alpha="0"
            android:layout_centerVertical="true"
            android:layout_toRightOf="@+id/cobros"
            android:layout_toEndOf="@+id/cobros"
            android:layout_marginLeft="5dp" />

        <Button
            android:layout_width="60dp"
            android:layout_height="wrap_content"
            android:text="entrega"
            android:id="@+id/entrega"
            android:alpha="0"
            android:layout_centerVertical="true"
            android:layout_toRightOf="@+id/desembolso"
            android:layout_toEndOf="@+id/desembolso"
            android:layout_marginLeft="68dp" />

    </RelativeLayout>

</RelativeLayout>
